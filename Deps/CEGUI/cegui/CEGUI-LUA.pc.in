moduledir=${prefix}/@CEGUI_INSTALL_MODULE_DIR@
datafiles=${prefix}/@CEGUI_INSTALL_DATA_DIR@

Name: CEGUI-@CEGUI_VERSION_MAJOR@ CEGUILuaScriptModule
